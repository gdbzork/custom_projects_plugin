<ul>
<% @psets[@pstack[-1]].each do |p| %>
  <li><a href="<%= project_url(p) %>"><%= @projects[p].name %></a> [<a href="<%= project_issues_path(@projects[p]) %>">Issues</a>]</li>
  <% if @psets.has_key? p %>
    <% @pstack.append(p) %>
    <%= render :partial => 'plist' %>
    <% @pstack.pop %>
  <% end %>
<% end %>
</ul>
